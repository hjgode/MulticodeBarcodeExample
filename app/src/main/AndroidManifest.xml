<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.honeywell.multiscanreader"
    android:versionCode="197000073"
    android:versionName="1.97.00.0073">

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="31" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/barcode_example_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.honeywell.multiscanreader.DataBaseView"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name="com.honeywell.multiscanreader.ClientBarcodeActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.honeywell.multiscanreader.AutomaticBarcodeActivity"
            android:exported="true"
            android:label="@string/barcode">
            <intent-filter>
                <action android:name="android.intent.action.AUTOMATICBARCODEACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.honeywell.multiscanreader.MainActivity"
            android:exported="true"
            android:label="@string/barcode">
            <intent-filter>
                <action android:name="android.intent.action.CLIENTBARCODEACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />

            </intent-filter>
        </activity>
        <activity
            android:name="com.honeywell.multiscanreader.ScannerSelectionBarcodeActivity"
            android:exported="true"
            android:label="@string/barcode">
            <intent-filter>
                <action android:name="android.intent.action.SCANNERSELECTBARCODEACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>